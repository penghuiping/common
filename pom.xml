<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.php25</groupId>
    <artifactId>common</artifactId>
    <version>2.0.3-SNAPSHOT</version>

    <modules>
        <module>common-parent</module>
        <module>common-spring-boot-starter</module>
        <module>core</module>
        <module>es</module>
        <module>db</module>
        <module>jpa</module>
        <module>mvc</module>
        <module>webflux</module>
        <module>redis</module>
        <module>sample/core-sample</module>
        <module>sample/db-sample</module>
        <module>sample/jpa-sample</module>
        <module>sample/redis-sample</module>
    </modules>

    <packaging>pom</packaging>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                    <encoding>UTF-8</encoding>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>